# cargar el archivo desde la pc 
uploaded = files.upload()

# Acceder al archivo cargado (asumiendo que solo cargas uno)
for filename in uploaded.keys():
    df = pd.read_csv(filename)

# mostrar los datos para comprobar 
print(df)

